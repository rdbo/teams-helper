{
    "manifest_version" : 2,
    "name" : "teams-helper",
    "description" : "Helper for Microsoft Teams",
    "version" : "1.0",
    "icons" : {
        "16"  : "img/icon_16.png",
        "32"  : "img/icon_32.png",
        "48"  : "img/icon_48.png",
        "128" : "img/icon_128.png"
    },

    "permissions" : [
        "tabs"
    ],

    "browser_action" : {
        "default_title" : "Teams Helper",
        "default_popup" : "popup.html",
        "default_icon" : "img/icon.png"
    }
}